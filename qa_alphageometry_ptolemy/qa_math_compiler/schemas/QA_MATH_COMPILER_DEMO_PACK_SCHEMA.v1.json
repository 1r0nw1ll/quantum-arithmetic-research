{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "QA_MATH_COMPILER_DEMO_PACK_SCHEMA.v1",
  "title": "QA_MATH_COMPILER_DEMO_PACK_SCHEMA.v1",
  "description": "Index schema for QA Math Compiler demo_pack_v1.",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "schema_id",
    "version",
    "example_count",
    "examples"
  ],
  "properties": {
    "schema_id": {
      "const": "QA_MATH_COMPILER_DEMO_PACK_SCHEMA.v1"
    },
    "version": {
      "type": "string",
      "minLength": 1
    },
    "example_count": {
      "type": "integer",
      "minimum": 1
    },
    "examples": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "id",
          "topic",
          "difficulty",
          "status"
        ],
        "properties": {
          "id": {
            "type": "string",
            "minLength": 1
          },
          "topic": {
            "type": "string",
            "minLength": 1
          },
          "difficulty": {
            "type": "string",
            "minLength": 1
          },
          "status": {
            "type": "string",
            "enum": [
              "PROVED",
              "UNPROVED",
              "AMBIGUOUS"
            ]
          }
        }
      }
    }
  }
}
